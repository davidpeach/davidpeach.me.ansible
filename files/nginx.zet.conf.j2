server {
        listen {{ http_port }};
        root /var/www/{{ http_host_zet }};
        index index.php index.html index.htm index.nginx-debian.html;
        server_name {{ http_host_zet }};

        location / {
                try_files $uri $uri/ =404;
        }

        location ~ /\.ht {
                deny all;
        }
}
